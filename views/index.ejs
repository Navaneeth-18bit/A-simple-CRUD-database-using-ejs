<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

</head>
<body>
    <div class="container">
    <h1>CRUD Operation</h1>
    <div>
    <button onclick="window.location.href='/Create'">+Add Data</button>
    </div>

    <div>
    <button onclick="window.location.href='/Update'">Update Data</button>
    </div>

    <div>
    <button onclick="window.location.href='/Delete'">Delete Data</button>
    </div>
    </div>
    <hr>

    <div>
        <table>
            <thead>
                <tr>
                    <th>
                        Student ID
                    </th>
                    <th>
                        Student Name
                    </th>
                    <th>
                        Room No
                    </th>

                </tr>
            </thead>
            <tbody>
                <% hostel.forEach((hostel) => {%>
                    <tr>
                        <td><%= hostel.student_id %></td>
                        <td><%= hostel.student_name %></td>
                        <td><%= hostel.room_no %></td>
                        
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>



</body>
</html>
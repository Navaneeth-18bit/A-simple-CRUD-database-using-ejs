<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Deletion</title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <th>
                        Student ID
                    </th>
                    <th>
                        Student Name
                    </th>
                    <th>
                        Room No
                    </th>
                    <th>
                        Action
                    </th>

                </tr>
            </thead>
            <tbody>
                <% hostel.forEach((hostel) => {%>
                    <tr>
                        <td><%= hostel.student_id %></td>
                        <td><%= hostel.student_name %></td>
                        <td><%= hostel.room_no %></td>
                        <td><button>Delete</button></td>
                        
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>

</body>
</html>